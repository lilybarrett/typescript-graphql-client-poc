{
  "name": "typescript-react-graphql-client-poc",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prebuild": "tsc",
    "dev": "concurrently \"tsc --pretty --watch\" \"node ./clients/server.js\"",
    "build": "next build",
    "introspect": "apollo-codegen introspect-schema http://localhost:4000/graphql --output ./models/schema/index.json",
    "generate": "apollo-codegen generate ./pages/**/data/**/*.ts --schema ./models/schema/index.json --target typescript --output ./models/schema/index.ts --add-typename --tag-name gql",
    "typegen": "npm run introspect && npm run generate",
    "start": "next start",
    "lint": "tslint --fix --project ./"
  },
  "dependencies": {
    "@zeit/next-sass": "^0.1.2",
    "@zeit/next-typescript": "^0.1.1",
    "apollo-cache-inmemory": "^1.2.0",
    "apollo-client": "^2.3.0",
    "apollo-client-preset": "^1.0.8",
    "apollo-codegen": "^0.19.1",
    "apollo-link-http": "^1.5.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.5.1",
    "cross-fetch": "^1.1.1",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^2.24.0",
    "next": "^6.0.0",
    "next-apollo": "^1.0.13",
    "next-routes": "^1.4.1",
    "react": "^16.3.2",
    "react-apollo": "^2.1.3",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.3.2",
    "recompose": "^0.27.0",
    "reflect-metadata": "^0.1.12",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/next": "^2.4.9",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "concurrently": "^3.5.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  }
}
